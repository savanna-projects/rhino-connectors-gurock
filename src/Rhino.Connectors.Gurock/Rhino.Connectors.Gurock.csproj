<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0</TargetFrameworks>
		<PackageId>Rhino.Connectors.Gurock</PackageId>
		<LangVersion>latest</LangVersion>
		<Title>Rhino TestRail Connector - Connector for Gurock Test Rail.</Title>
		<Authors>Rhino-API Community</Authors>
		<Description>Connector for Gurock TestRail and Jira for bugs management.</Description>
		<PackageTags>automation,crawling,crawl,selenium,appium,automation testing,automation test,qa,gravity,gravity api,scrapping,scrap</PackageTags>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/savanna-projects/rhino-connectors-gurock</PackageProjectUrl>
		<NeutralLanguage>en-US</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Rhino.Api" Version="2022.3.14.1" />
		<PackageReference Include="Rhino.Connectors.AtlassianClients" Version="2022.3.14.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Rhino.Connectors.GurockClients\Rhino.Connectors.GurockClients.csproj" />
	</ItemGroup>

</Project>
